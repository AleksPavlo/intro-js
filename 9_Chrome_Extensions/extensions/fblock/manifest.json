{
  "name": "Post BLOCK",
  "description": "Remove Unwanted Posts from News Feeds",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [ "storage", "activeTab", "scripting", "tabs" ],
  "action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  },
  "host_permissions": [ "*://*/*" ]
}
